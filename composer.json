{
	"name": "diskerror/twitter-digester",
	"description": "Consumes a Twitter stream and counts the hashtags, for starters...",
	"type": "project",
	"license": "public",
	"homepage": "http://www.politicator.com/",
	"authors": [{
		"name": "Reid Woodbury Jr",
		"email": "reid@rawsound.com",
		"homepage": "http://www.rawsound.com",
		"role": "Developer"
	}],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "^7.1",
		"ext-phalcon": "^3.3",
		"ext-intl": "*",
		"ext-mbstring": "*",
		"diskerror/typedbson": "dev-master",
		"mongodb/mongodb": "^1.4",
		"zendframework/zend-json": "^3.0",
		"zendframework/zend-stdlib": "^2.7 || ^3.0",
		"php-ds/php-ds": "^1.2 || ^2.0",
		"php-science/textrank": "^1.0",
		"zendframework/zend-server": "^2.8.0"
	},
	"suggest": {
		"ext-ds": "*"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"phalcon/ide-stubs": "3.3"
	},
	"autoload": {
		"psr-4": {
			"Code\\": "app/models/code/",
			"Resource\\": "app/models/Resource/",
			"Structure\\": "app/models/Structure/"
		}
	}
}
