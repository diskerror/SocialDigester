{
	"name": "diskerror/twitter-digester",
	"description": "Consumes a Twitter stream and counts the hashtags, for starters...",
	"type": "project",
	"license": "public",
	"homepage": "http://www.politicator.com/",
	"authors": [
		{
			"name": "Reid Woodbury Jr",
			"email": "reid@rawsound.com",
			"homepage": "http://www.rawsound.com",
			"role": "Developer"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "^7.4",
		"ext-phalcon": "^4.1",
		"ext-curl": "*",
		"ext-intl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-mongodb": "1.12.*",
		"ext-oauth": "*",
		"ext-pcntl": "*",
		"ext-shmop": "*",
		"diskerror/autoload": "dev-main",
		"diskerror/typedbson": "dev-master",
		"laminas/laminas-code": "4.4.*",
		"laminas/laminas-json": "^3.3",
		"laminas/laminas-server": "^2.10.0",
		"laminas/laminas-stdlib": "^3.6",
		"mongodb/mongodb": "^1.8",
		"php-ds/php-ds": "^1.3",
		"php-science/textrank": "^1.2"
	},
	"suggest": {
		"ext-ds": "*"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"phalcon/ide-stubs": "^4.1"
	},
	"autoload": {
		"classmap": ["app/tasks", "app/controllers"],
		"psr-4": {
			"Logic\\": "app/models/logic",
			"Resource\\": "app/models/resource",
			"Service\\": "app/models/service",
			"Structure\\": "app/models/struct"
		}
	}
}
